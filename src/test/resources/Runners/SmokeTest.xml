<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" verbose="4">
	<test thread-count="5" name="Productstest">
	<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="com.qa.OpenKart.testCases.ProductsTest" />
		</classes>
	</test> 
	
	<test thread-count="5" name="success">
	<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.qa.OpenKart.testCases.OrderSuccessTest" />
		</classes>
	</test>
</suite> 
